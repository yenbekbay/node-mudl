{
  "name": "mudl",
  "version": "0.2.0",
  "description": "A cli tool for downloading music from VK in high quality",
  "keywords": [
    "music",
    "download",
    "vk",
    "audio",
    "mp3"
  ],
  "repository": "/yenbekbay/node-mudl.git",
  "homepage": "https://github.com/yenbekbay/node-mudl#readme",
  "bugs": {
    "url": "https://github.com/yenbekbay/node-mudl/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Ayan Yenbekbay",
    "email": "ayan.yenb@gmail.com",
    "url": "http://yenbekbay.me"
  },
  "engines": {
    "node": ">=5.0",
    "npm": ">=3.0"
  },
  "main": "./lib/cli.js",
  "preferGlobal": true,
  "bin": {
    "mudl": "./lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "commit": "git-cz",
    "build": "npm run clean && babel --out-dir ./lib --ignore **/__tests__,**/__mocks__ ./src",
    "clean": "rimraf lib && mkdir lib",
    "prepublish": "npm run build",
    "test": "npm-run-all --parallel typecheck lint jest",
    "jest": "jest",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "typecheck": "flow check ./src",
    "commitmsg": "validate-commit-msg",
    "prepush": "npm test",
    "release": "standard-version"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testRegex": "/__tests__/.*-test\\.js$"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "maxSubjectLength": 72
    }
  },
  "dependencies": {
    "browser-id3-writer": "^2.3.1",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "configstore": "^2.0.0",
    "email-regex": "^1.0.0",
    "gauge": "^2.6.0",
    "he": "^1.1.0",
    "inquirer": "^1.1.2",
    "ipics": "^1.0.1",
    "lodash": "^4.13.1",
    "ora": "^0.3.0",
    "path-exists": "^3.0.0",
    "request": "^2.76.0",
    "request-progress": "^2.0.1",
    "request-promise": "^4.1.1",
    "similarity": "^1.1.0",
    "update-notifier": "^1.0.2",
    "yargs": "^6.3.0"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.30",
    "@types/cheerio": "^0.17.31",
    "@types/configstore": "^0.3.30",
    "@types/he": "^0.5.29",
    "@types/inquirer": "^0.0.29",
    "@types/jest": "^15.1.32",
    "@types/lodash": "^4.14.37",
    "@types/ora": "^0.0.29",
    "@types/path-exists": "^1.0.29",
    "@types/request": "^0.0.31",
    "@types/request-promise": "^3.0.32",
    "@types/update-notifier": "^0.0.28",
    "@types/yargs": "^0.0.33",
    "babel-cli": "^6.18.0",
    "babel-jest": "^17.0.0",
    "babel-plugin-lodash": "^3.2.8",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.5.0",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.8.1",
    "eslint-config-anvilabs": "^2.0.1",
    "flow-bin": "^0.33.0",
    "husky": "^0.11.9",
    "jest": "^17.0.0",
    "npm-run-all": "^3.1.0",
    "rimraf": "^2.5.4",
    "standard-version": "^3.0.0",
    "validate-commit-msg": "^2.8.2"
  }
}
